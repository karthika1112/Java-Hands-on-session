package forloop;

public class forloop {

	public static void main(String[] args) {
		String name = "karthika";
		
		for(int i=1;i<=5;i++)
		{
			System.out.println(name);
		}
		
		for(int i=1;i<=10;i++)
		{
			System.out.print(" "+i);
		}
		System.out.println("");

		for(int i=10;i>=1;i--)
		{
			System.out.print(" "+i);
		}

	}

}
