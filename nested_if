package nested_if;

public class Nested_if {

	public static void main(String[] args) {
		boolean kfc = true;
		boolean chicken = true;
		boolean pepsi = true;
		if(kfc)
		{
			System.out.println("Enter into KFC");
			
			if(chicken) {
				System.out.println("Eadting chicken");
				
				if(pepsi)
				{
					System.out.println("Drinking pepsi");
				}
			}
			
		}

	}

}
