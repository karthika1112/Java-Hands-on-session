import java.util.Scanner;
class Main 
{
    public static void main(String[] args) 
    {
        int num;
        Scanner obj= new Scanner(System.in);
        System.out.print("Enter the number : ");
        num=obj.nextInt();
        obj.nextLine();
        System.out.println("Number : "+num);
        if(num%2==0)
           System.out.println("Even");
        else
           System.out.println("Odd");
    }
}
