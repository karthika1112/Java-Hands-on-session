package whileloop;
import java.util.Random;
public class whileloop {

    public static void main(String[] args) {
        Random rand = new Random();
        int newnum = 0;
        while(newnum!=5) {
        	newnum = rand.nextInt(11);
        	System.out.println(newnum);
        }
    }
}
