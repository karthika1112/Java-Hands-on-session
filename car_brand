package Car;

class Car {
    String color = "black";

    void display() {
        System.out.println("The color of car: " + color);
    }
}

class Honda extends Car {
   
}

class BMW extends Car {
   
}

class Skoda extends Car {
   
}

public class carbrand {
    public static void main(String[] args) {
        Car ob1 = new Honda();
        ob1.color = "blue";
        ob1.display();  

        Car ob2 = new BMW();
        ob2.color = "red";
        ob2.display();  

        Car ob3 = new Skoda();
        ob3.color = "white";
        ob3.display(); 
    }
}
